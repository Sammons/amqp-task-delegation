{
  "rules": {
    "no-unused-variable": [
      true
    ],
    "class-name": true,
    "curly": true,
    "eofline": false,
    "max-line-length": [
      true,
      120
    ],
    "forin": true,
    "variable-name": [
      "ban-keywords",
      "allow-pascal-case",
      true
    ],
    "arrow-parens": true
  }
}